<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SignupFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:padding="16dp">

        <EditText
            android:id="@+id/etUsernameSignup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="username...."
            android:inputType="textEmailAddress"
            android:minHeight="48dp"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="275dp"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etPasswordSignup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="password...."
            android:inputType="textPassword"
            android:minHeight="48dp"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="338dp"
            tools:ignore="MissingConstraints" />

        <Button
            android:id="@+id/btnSignupSignup"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:backgroundTint="#6F7F8F"
            android:text="SIGNUP"
            tools:layout_editor_absoluteX="230dp"
            tools:layout_editor_absoluteY="626dp"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etFirstnameSignupFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="firstname..."
            android:inputType="text"
            android:minHeight="48dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="153dp" />

        <EditText
            android:id="@+id/etLastnameSignupFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="lastname..."
            android:inputType="text"
            android:minHeight="48dp"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="221dp"
            tools:ignore="MissingConstraints" />

        <EditText
            android:id="@+id/etConfirmPasswordSignupFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="confirm password..."
            android:inputType="textPassword"
            android:minHeight="48dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="101dp"
            tools:layout_editor_absoluteY="382dp" />

        <EditText
            android:id="@+id/etAddressSignupFragment"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:ems="10"
            android:hint="address..."
            android:inputType="text"
            android:minHeight="48dp"
            tools:ignore="MissingConstraints"
            tools:layout_editor_absoluteX="100dp"
            tools:layout_editor_absoluteY="429dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>
